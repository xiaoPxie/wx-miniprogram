.pyg_title {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1;
  transition: background .5s;
}
.pyg_title .title {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
  color: #fff;
  font-size: 32rpx;
}
.idx_swiper {
  height: 340rpx;
}
.idx_swiper swiper {
  height: inherit;
}
.idx_swiper_nav {
  height: 100%;
}
.idx_swiper_img {
  width: 100%;
}
.idx_cates {
  display: flex;
}
.idx_cates navigator {
  flex: 1;
  padding: 20rpx;
}
/* .idx_cates image {
  width: 100%;
} */
.idx_floor {
  
}
.idx_floor .floor_group{
  margin: 10rpx 10rpx 0;
}
.floor_group .floor_title{

}
/* .floor_group .floor_title image{
  width: 100%;
} */
.floor_group .floor_list {
  overflow: hidden;
}
.floor_group .floor_list navigator{
  float: left;
  width: calc(calc(100vw - 10rpx * 4) / 3);
  /* height: calc(calc(33.333333vw - 10rpx * 2) * 386 / 232); */
}
.floor_group .floor_list navigator:nth-of-type(2),
.floor_group .floor_list navigator:nth-of-type(3),
.floor_group .floor_list navigator:nth-of-type(4),
.floor_group .floor_list navigator:nth-of-type(5){
  /* 第一张长图的高度
  232 * 386 = 33.33vw * height
  => height = 33.33vw * 386 / 232 */
  /* height: calc(33.33vw * 386 / 232); */
  height: calc(calc(calc(calc(100vw - 10rpx * 4) / 3) * 386 / 232 - 10rpx) / 2);
}
.floor_group .floor_list navigator:nth-of-type(2),
.floor_group .floor_list navigator:nth-of-type(4){
  margin: 0 10rpx;
}
.floor_group .floor_list navigator:nth-of-type(2),
.floor_group .floor_list navigator:nth-of-type(3){
  margin-bottom: 10rpx;
}
.floor_group .floor_list navigator image {
  /* width: 100%; */
  height: 100%;
}